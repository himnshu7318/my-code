#hashsets also called sets in python
s= set()
s.add(2)
s.add(4)
s.add(6)
print(s)
#look up 
if 2 in  s:
  print(True)
    
s.remove(4)
print(s)


#hashmaps also called dictionary in python
d={}
d={'greg':1,'steve':2,'tom':3}
print(d)
#add
d['arsh']=4
print(d)
#lookup
if 'greg' in d:
    print(True)
#loopover wit key and value
for key,val in d.items():
    print(f"key{key}:val{val}")
    
#counter
string="aaaaaaabbbbbbbcccchhhhhdddduuuueeeefffff"
from collections import Counter
count = Counter(string)
print(count)
